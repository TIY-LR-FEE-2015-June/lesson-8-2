<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Address</th>
      <th></th>
    </tr>
  </thead>
  {{#each model as |contact|}}
    <tr>
      <td>{{contact.fullName}}</td>
      <td>{{contact.phone}}</td>
      <td>{{contact.address}}</td>
      <td>
        {{#link-to 'contacts.edit' contact}}Edit{{/link-to}}
      </td>
    </tr>
  {{/each}}
</table>
